<?xml version="1.0" encoding="UTF-8"?>
<sdf version="1.5">
   <world name="default">
      <!-- A global light source -->
      <include>
         <uri>model://sun</uri>
      </include>
      <gravity>0 0 0</gravity>
       <model name="DpSphere">
               <pose>0 0 0 0 0 0</pose>
               <link name="link">
                  <visual name="visual">
                     <transparency>1.0</transparency>
                     <geometry>
                        <sphere>
                           <radius>0.01</radius>
                        </sphere>
                     </geometry>
                     <material>
                        <ambient>0.5 1 1 1</ambient>
                        <diffuse>0.5 1 1 1</diffuse>
                        <specular>0.1 0.1 0.1 1</specular>
                        <emissive>0 0 0 0</emissive>
                     </material>
                  </visual>
               </link>
       </model>
      <model name="ScopeTipSphere">
         <pose>0 0 0 0 0 0</pose>
         <link name="link">
            <visual name="visual">
               <transparency>1.0</transparency>
               <geometry>
                  <sphere>
                     <radius>0.01</radius>
                  </sphere>
               </geometry>
               <material>
                  <ambient>0.5 1 1 1</ambient>
                  <diffuse>0.5 1 1 1</diffuse>
                  <specular>0.1 0.1 0.1 1</specular>
                  <emissive>0 0 0 0</emissive>
               </material>
            </visual>
         </link>
      </model>
      <model name="RCMSphere">
         <pose>0 0 0 0 0 0</pose>
         <link name="link">
            <visual name="visual">
               <transparency>0.0</transparency>
               <geometry>
                  <sphere>
                     <radius>0.01</radius>
                  </sphere>
               </geometry>
               <material>
                  <ambient>0.5 0 0 1</ambient>
                  <diffuse>0.5 1 1 1</diffuse>
                  <specular>0.1 0.1 0.1 1</specular>
                  <emissive>0 0 0 0</emissive>
               </material>
            </visual>
         </link>
      </model>
      <model name="EndEffectorSphere">
         <pose>0 0 0 0 0 0</pose>
         <link name="link">
            <visual name="visual">
               <transparency>0.0</transparency>
               <geometry>
                  <sphere>
                     <radius>0.01</radius>
                  </sphere>
               </geometry>
               <material>
                  <ambient>0.5 1 1 1</ambient>
                  <diffuse>0.5 1 1 1</diffuse>
                  <specular>0.1 0.1 0.1 1</specular>
                  <emissive>0 0 0 0</emissive>
               </material>
            </visual>
         </link>
      </model>
      <model name="ScopeCylinder">
         <pose>0 0 0 0 0 0</pose>
         <link name="cylinder_link">
            <visual name="visual">
               <transparency>0.0</transparency>
               <geometry>
                  <cylinder>
                     <radius>0.005</radius>
                     <length>0.02</length>
                  </cylinder>
               </geometry>
               <material>
                  <ambient>0.5 1 1 1</ambient>
                  <diffuse>0.5 1 1 1</diffuse>
                  <specular>0.1 0.1 0.1 1</specular>
                  <emissive>0 0 0 0</emissive>
               </material>
            </visual>
         </link>
         <link name="camera_link">
            <pose>0 0 0 3.14159 -1.5708 1.5708</pose>
            <sensor type="camera" name="camera1">
               <update_rate>300.0</update_rate>
               <visualize>true</visualize>
               <camera name="head">
                  <horizontal_fov>1.3962634</horizontal_fov>
                  <image>
                     <width>800</width>
                     <height>800</height>
                     <format>R8G8B8</format>
                  </image>
                  <clip>
                     <near>0.02</near>
                     <far>300</far>
                  </clip>
               </camera>
               <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
                  <cameraName>/cylinder_camera</cameraName>
                  <alwaysOn>true</alwaysOn>
               </plugin>
            </sensor>
         </link>
      </model>

      <model name="ScopeCylinder2">
         <pose>0 0 0 0 0 0</pose>
         <link name="cylinder_link">
            <visual name="visual">
               <transparency>0.0</transparency>
               <geometry>
                  <cylinder>
                     <radius>0.005</radius>
                     <length>0.02</length>
                  </cylinder>
               </geometry>
               <material>
                  <ambient>0.5 0.5 0.5 0.5</ambient>
                  <diffuse>0.5 0.5 0.5 1</diffuse>
                  <specular>0.1 0.1 0.1 1</specular>
                  <emissive>0 0 0 0</emissive>
               </material>
            </visual>
         </link>
         <link name="camera_link">
            <pose>0 0 0 3.14159 -1.5708 1.5708</pose>
            <sensor type="camera" name="camera2">
               <update_rate>300.0</update_rate>
               <visualize>true</visualize>
               <camera name="head">
                  <horizontal_fov>1.3962634</horizontal_fov>
                  <image>
                     <width>800</width>
                     <height>800</height>
                     <format>R8G8B8</format>
                  </image>
                  <clip>
                     <near>0.02</near>
                     <far>300</far>
                  </clip>
               </camera>
               <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
                  <cameraName>/cylinder_camera2</cameraName>
                  <alwaysOn>true</alwaysOn>
               </plugin>
            </sensor>
         </link>
      </model>
      <model name="rigcage_mesh">
         <pose>-0.5 -0.3 0.1 0 0 -1.57</pose>
         <static>true</static>
         <link name="body">
            <visual name="visual">
               <geometry>
                  <mesh>
                     <uri>model://meshes/ribcage.dae</uri>
                     <scale>0.0012 0.0012 0.0012</scale>
                  </mesh>
               </geometry>
               <material>
                  <script>
                     <uri>model://materials/scripts/ribcage.material</uri>
                     <name>RibCageTexture</name>
                  </script>
               </material>
            </visual>
         </link>
      </model>
      <model name="curve">
         <pose>0 -0.12 -0.12 0 0 0</pose>
         <link name="body">
            <visual name="visual">
               <geometry>
                  <mesh>
                     <uri>model://meshes/curve_transformed.stl</uri>
                     <scale>0.0025 0.0025 0.0025</scale>
                  </mesh>
               </geometry>
               <material>
                  <ambient>0.5 1 1 1</ambient>
                  <diffuse>0.5 1 1 1</diffuse>
                  <specular>0.1 0.1 0.1 1</specular>
                  <emissive>0 0 0 0</emissive>
               </material>
            </visual>
         </link>
      </model>
   </world>
</sdf>
